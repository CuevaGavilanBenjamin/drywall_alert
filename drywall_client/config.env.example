# DryWall Client Configuration
# Copy this file to config.env and edit the values

# SFTP Configuration
SFTP_HOST=your-sftp-server.com
SFTP_PORT=22
SFTP_USERNAME=drywall_user
SFTP_PASSWORD=your-password
SFTP_REMOTE_PATH=/upload

# ERP API Configuration
ERP_API_URL=http://your-erp-server.com:8000

# Data Generation Settings
HUMIDITY_SAMPLES=100
DATA_FORMAT=csv

# Monitoring Settings
LOG_LEVEL=INFO
